{
  "name": "parcel",
  "description": "Include Bootstrap's source Sass and individual JavaScript plugins with Parcel.",
  "version": "0.0.0",
  "private": true,
  "repository": "twbs/examples",
  "license": "MIT",
  "homepage": "https://yuliia-anisimova.github.io/bootstrap-tutorial/",
  "stackblitz": {
    "startCommand": "npm start"
  },
  "scripts": {
    "start": "parcel serve src/index.html --public-url / --dist-dir dist",
    "build": "parcel build src/index.html --public-url / --dist-dir dist",
    "deploy": "npm run build && gh-pages -d dist -b main -o '/bootstrap-tutorial/'"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "gh-pages": "^6.0.0",
    "parcel": "^2.8.3"
  }
}
